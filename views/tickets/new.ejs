<%- include('../partials/header') %>


<form id="add-ticket-form" action="/flights/<%= flightId %>tickets" method="POST">
  <br><br><label>Seat:</label>
  <input type="text" name="seat" required pattern="[A-F][1-9]\d?">
  <br><br><label>Price:</label>
  <input type="number" name="price" min="0">
  <br><br><label>Flight Number:</label>
  <input type="text" name="flightNo" value="<%= flightId %>">
  <br><br><input type="submit" value="Add Ticket">
</form>

<%- include('../partials/footer') %>